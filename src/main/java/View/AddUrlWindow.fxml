<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
        <?import javafx.geometry.*?>
        <?import javafx.scene.control.*?>

<BorderPane prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.UrlWindowController">
<center>
    <GridPane hgap="5.0" vgap="5.0" BorderPane.alignment="CENTER">
        <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" />
            <RowConstraints minHeight="10.0" />
        </rowConstraints>
        <children>
            <Label text="Url">
                <GridPane.margin>
                    <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                </GridPane.margin>
            </Label>


            <TextField fx:id="urlTextField" GridPane.columnIndex="1">
                <GridPane.margin>
                    <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                </GridPane.margin>
            </TextField>


        </children>
    </GridPane>
</center>
<bottom>
    <HBox alignment="BOTTOM_RIGHT" spacing="5.0" BorderPane.alignment="CENTER">
        <children>
            <Button mnemonicParsing="false" onAction="#handleCancelAction" prefWidth="90.0" text="Cancel">
                <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#handleAdd" prefWidth="90.0" text="Add" >
                <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </HBox.margin></Button>
        </children>

        <BorderPane.margin>
            <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
        </BorderPane.margin>
    </HBox>
</bottom>
<opaqueInsets>
    <Insets />
</opaqueInsets>
</BorderPane>